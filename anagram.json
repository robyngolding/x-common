{
    "#": [
        "The string argument cases possible matches are passed in as",
        "individual arguments rather than arrays. Languages can include",
        "these string argument cases if passing individual arguments is",
        "idiomatic in that language.",
        "Some tests have a \"comment\" property, which provides more",
        "information that could be added to the test case as a code comment."
    ],
    "cases": [
        {
            "description": "no matches",
            "subject": "diaper",
            "possible matches": [ "hello", "world", "zombies", "pants"],
            "expected": []
        },
        {
            "description": "detects simple anagram",
            "subject": "ant",
            "possible matches": ["tan", "stand", "at"],
            "expected": ["tan"]
        },
        {
            "description": "does not detect false positives",
            "subject": "galea",
            "possible matches": ["eagle"],
            "expected": []
        },
        {
            "description": "detects multiple anagrams",
            "subject": "master",
            "possible matches": ["stream", "pigeon", "maters"],
            "expected": ["stream", "maters"]
        },
        {
            "description": "does not detect anagram subsets",
            "subject": "good",
            "possible matches": ["dog", "goody"],
            "expected": []
        },
        {
            "description": "detects anagram",
            "subject": "listen",
            "possible matches": ["enlists", "google", "inlets", "banana"],
            "expected": ["inlets"]
        },
        {
            "description": "detects multiple anagrams",
            "subject": "allergy",
            "possible matches": ["gallery", "ballerina", "regally", "clergy", "largely", "leading"],
            "expected": ["gallery", "regally", "largely"]
        },
        {
            "description": "does not detect indentical words",
            "subject": "corn",
            "possible matches": ["corn", "dark", "Corn", "rank", "CORN", "cron", "park"],
            "expected": ["cron"]
        },
        {
            "description": "does not detect non-anagrams with identical checksum",
            "subject": "mass",
            "possible matches": ["last"],
            "expected": []
        },
        {
            "description": "detects anagrams case-insensitively",
            "subject": "Orchestra",
            "possible matches": ["cashregister", "Carthorse", "radishes"],
            "expected": ["Carthorse"]
        },
        {
            "description": "detects anagrams using case-insensitve subject",
            "subject": "Orchestra",
            "possible matches": ["cashregister", "carthorse", "radishes"],
            "expected": ["carthorse"]
        },
        {
            "description": "detects anagrams using case-insensitve possible matches",
            "subject": "orchestra",
            "possible matches": ["cashregister", "Carthorse", "radishes"],
            "expected": ["carthorse"]
        },
        {
            "description": "does not detect a word as its own anagram",
            "subject": "banana",
            "possible matches": ["Banana"],
            "expected": []
        },
        {
            "description": "detects an anagram but not the original word",
            "subject": "corn",
            "possible matches": ["corn" "dark" "Corn" "rank" "CORN" "cron" "park"],
            "expected": ["cron"]
        },
        {
            "description": "does not detect a anagram if the original word is repeated",
            "subject": "go",
            "possible matches": ["go Go GO"],
            "expected": []
        },
        {
            "description": "anagrams must use all letters exactly once",
            "subject": "tapper",
            "possible matches": ["patter"],
            "expected": []
        },
        {
            "description": "eliminates anagrams with the same checksum",
            "subject": "mass",
            "possible matches": ["last"],
            "expected": []
        },
        {
            "description": "detects unicode anagrams",
            "comment": "These words don't make sense, they're just greek letters cobbled together.",
            "subject": "ΑΒΓ",
            "possible matches": ["ΒΓΑ", "ΒΓΔ", "γβα"],
            "expected": ["ΒΓΑ", "γβα"]
        },
        {
            "description": "eliminates misleading unicode anagrams",
            "comment": "Despite what a human might think these words different letters, the input uses Greek A and B while the list of potential anagrams uses Latin A and B.",
            "subject": "ΑΒΓ",
            "possible matches": ["ABΓ"],
            "expected": []
        },
        {
            "description": "capital word is not own anagram",
            "subject": "BANANA",
            "possible matches": ["Banana"],
            "expected": []

        },
        {
            "description": "anagrams must use all letters exactly once",
            "subject": "patter",
            "possible matches": ["tapper"],
            "expected": []
        }
    ],
    "string argument cases": [
        {
            "description": "accepts string arguments",
            "subject": "ant",
            "possible matches": ["stand", "tan", "at"],
            "expected": ["tan"]
        },
        {
            "description": "accepts single string argument",
            "subject": "ant",
            "possible matches": ["tan"],
            "expected": ["tan"]
        },
    ]
}